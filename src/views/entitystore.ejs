<h1>Type page for <%= typeStr %></h1>
<%- include("partials/entitystore/storesgrid.ejs", {typeStores: typeStores}) %>
<% if (entityStore !== null && entityStore !== undefined) { %>
  <h2><%= entityStore.name %></h2>
  <p>
    <a href="/entitystore/<%= entityStore.id %>/edit">Edit</a>
    <a href="/entitystore/<%= entityStore.id %>/delete"
      onclick="return confirm('Are you sure?')">Delete</a>
  </p>
  <h2>Contents</h2>
  <% if (entityStore.entities && entityStore.entities.length > 0) { %>
    <%- include("partials/entitystore/entities.ejs", {entities: entityStore.entities}) %>
  <% } else { %>
    <p>Nothing yet</p>
  <% } %>
  <p>
    <a href="/entity/add/type/<%= typeStr %>">Add a <%= typeStr %></a>
  </p>
<% } %>